generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model blocks {
  id                                      Int       @id @default(autoincrement())
  project_id                              Int
  block_name                              String?
  development_start_date                  DateTime? @db.Date
  development_end_date                    DateTime? @db.Date
  height                                  Int?
  fsi                                     Int?
  built_up_area                           Int?
  commencement_certificate_no             String?
  date_of_latest_commencement_certificate DateTime? @db.Date
  all_commencement_certificates           String?
  projects                                projects  @relation(fields: [project_id], references: [id], onDelete: NoAction, onUpdate: NoAction)
}

model test {
  id     Int     @id
  name   String? @db.VarChar(20)
  gender String? @db.Char(1)
}

model projects {
  id               Int                @id @default(autoincrement())
  name             String?            @db.VarChar(50)
  ProjectExtension ProjectExtension[]
  blocks           blocks[]
}

model ProjectExtension {
  id                               Int      @id @default(autoincrement())
  extendToDate                     DateTime @default(now())
  reasonForExtension               String[]
  supportingDocuments              String?
  supportingDocumentsOptions       String?
  documentDate                     DateTime @default(now())
  declarationFormB                 String?
  authorizedSignatory              String?
  contactDetails                   String?
  DevelopmentDateB1                DateTime @default(now())
  DevelopmentDeclarationFormB1     String?
  DevelopmentAuthorizedSignatoryB1 String?
  contactB1                        String?
  DevelopmentDateB2                DateTime @default(now())
  DevelopmentDeclarationFormB2     String?
  LandOwnerB2                      String?
  contactB2                        String?
  order45                          String?
  project_id                       Int
  projects                         projects @relation(fields: [project_id], references: [id])
}
