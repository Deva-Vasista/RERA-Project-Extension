generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model blocks {
  id                                      String    @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  project_id                              String?   @db.Uuid
  block_name                              String?
  development_start_date                  DateTime? @db.Date
  development_end_date                    DateTime? @db.Date
  height                                  Int?
  fsi                                     Int?
  built_up_area                           Int?
  commencement_certificate_no             String?
  date_of_latest_commencement_certificate DateTime? @db.Date
  all_commencement_certificates           String?
}

model form_b {
  id                     String    @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  project_id             String?   @db.Uuid
  date_of_issuance       DateTime? @db.Date
  declaration_form_b     Bytes?
  authorized_signatory   String?
  contact_details        String?
  new_declaration_form_b Bytes?
}

model form_b1 {
  id                      String    @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  project_id              String?   @db.Uuid
  date_of_issuance        DateTime? @db.Date
  declaration_form_b1     Bytes?
  authorized_signatory    String?
  contact_details         String?
  new_declaration_form_b1 Bytes?
}

model form_b2 {
  id                      String    @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  project_id              String?   @db.Uuid
  date_of_issuance        DateTime? @db.Date
  declaration_form_b2     Bytes?
  authorized_signatory    String?
  contact_details         String?
  new_declaration_form_b2 Bytes?
  order_45                Bytes?
}

model projects {
  id                    String    @id @default(dbgenerated("uuid_generate_v4()")) @db.Uuid
  extend_to_date        DateTime? @db.Date
  reasons_for_extension String[]
  supporting_documents  String?
}

model test {
  id     Int     @id
  name   String? @db.VarChar(20)
  gender String? @db.Char(1)
}
